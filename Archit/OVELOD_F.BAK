#include<iostream.h>
#include<conio.h>
class emp
{
	public:int a,b;
	friend emp operator *(emp,emp);
	emp {}
	emp(int x,int y)
	{
		a=x;
		b=y;

	}
	public:void show()
	{
		cout<<"a=="<<a<<endl;
		cout<<"b=="<<b<<endl;

	}
};
emp  operator + (emp e , emp e1)
{
	emp obj;
	obj.a=e.a+e1.a;
	obj.b=e.b+e1.b;
	return obj;

}
